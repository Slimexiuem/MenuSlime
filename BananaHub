-- Gui to Lua
-- Version: 3.6

-- Instances:

local Bananahub = Instance.new("ScreenGui")
local stoptween = Instance.new("TextButton")
local Bananaicon = Instance.new("ImageButton")

-- Properties:

Bananahub.Name = "Bananahub"
Bananahub.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

stoptween.Name = "stoptween"
stoptween.Parent = Bananahub
stoptween.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
stoptween.BorderColor3 = Color3.fromRGB(0, 0, 0)
stoptween.BorderSizePixel = 0
stoptween.Position = UDim2.new(0, 0, 0.30662021, 0)
stoptween.Size = UDim2.new(0, 59, 0, 41)
stoptween.Font = Enum.Font.SourceSans
stoptween.Text = "Stop Tween"
stoptween.TextColor3 = Color3.fromRGB(0, 0, 0)
stoptween.TextScaled = true
stoptween.TextSize = 10.000
stoptween.TextWrapped = true

Bananaicon.Name = "Bananaicon"
Bananaicon.Parent = Bananahub
Bananaicon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Bananaicon.BorderColor3 = Color3.fromRGB(0, 0, 0)
Bananaicon.BorderSizePixel = 0
Bananaicon.Position = UDim2.new(0.177700356, 0, 0.524390221, 0)
Bananaicon.Size = UDim2.new(0, 52, 0, 49)
Bananaicon.Image = "rbxassetid://15981329344"

-- Scripts:

local function QVUW_fake_script() -- stoptween.Banfake2 
	local script = Instance.new('LocalScript', stoptween)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer:Kick("Bạn đã bị ban do bị hệ thống <AntiCheat> phát hienj sử dụng phần mềm thứ 3 98 năm 10 ngày")
	end)
end
coroutine.wrap(QVUW_fake_script)()
local function DAJZPZ_fake_script() -- Bananaicon.Banfaker 
	local script = Instance.new('LocalScript', Bananaicon)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer:Kick("Bạn đã bị ban do bị hệ thống <AntiCheat> phát hienj sử dụng phần mềm thứ 3 98 năm 10 ngày")
	end)
end
coroutine.wrap(DAJZPZ_fake_script)()
